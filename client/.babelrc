{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "chrome": "58"
        }
      }
    ],
    "@babel/react",
    [
      "@babel/preset-typescript",
      {
        "isTSX": true,
        "allExtensions": true
      }
    ]
  ],
  "plugins": ["@babel/proposal-class-properties"],
  "sourceMaps": "both"
}
